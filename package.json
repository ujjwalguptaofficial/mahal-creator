  "dependencies": {
    "cli-spinner": "^0.2.10",
    "commander": "^8.3.0",
    "fs-extra": "^10.0.0",
    "github-directory-downloader": "^1.3.6",
    "inquirer": "^8.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.12",
